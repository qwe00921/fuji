<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!-- 弹出框样式 -->
  <style name="pop_dialog"  parent="@android:style/Theme.Dialog">
     <item name="android:windowFrame">@null</item> <!-- 边框 -->
     <item name="android:windowIsFloating">true</item> <!--是否浮现在Dialog之上  -->
     <item name="android:windowIsTranslucent">false</item><!-- 半透明 -->
     <item name="android:windowNoTitle">true</item><!-- 无标题 -->
     <!--<item name="android:windowBackground">@null</item>  背景透明 -->
     <item name="android:windowBackground">@drawable/app_uncheck</item>   <!-- 背景透明 -->
     <item name="android:backgroundDimEnabled">true</item><!-- 模糊 -->
     <item name="android:windowContentOverlay">@null</item>
   </style>
   
   <!-- 标题背景样式 -->
   <style name="bg_top_background">
   	 <item name="android:layout_width">fill_parent</item>
   	 <item name="android:layout_height">50dip</item>
   	 <item name="android:background">@drawable/bg_top_new</item>
   	 <item name="android:textSize">24sp</item>
   </style>
   
   <!-- 首页分隔线 -->
   <style name="spline_main_draw_horizontal">
   	 <item name="android:layout_width">fill_parent</item>
   	 <item name="android:layout_height">0.3dip</item>
   	 <item name="android:background">@color/grey2</item>
   </style>
   <!-- 购物车分隔虚线 -->
   <style name="spline_shoppingcart_draw_horizontal">
   	 <item name="android:src">@drawable/line_dotted</item>
   	 <item name="android:layout_width">fill_parent</item>
   	 <item name="android:layout_height">wrap_content</item>
   </style>
   
   <!-- LoginButton样式 -->
   <style name="btn_Login">
