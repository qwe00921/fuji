package com.fsti.fjdicClient.bean;

/**
 * 弹窗分类实体
 * @author 金涛
 *
 */
public class PopSortEntity {
	//分类名
	private String popSortName;
	//分类ID，wang...
	private String popSortID;
	public String getPopSortID() {
		return popSortID;
	}
	public void setPopSortID(String popSortID) {
		this.popSortID = popSortID;
	}
	//分类值,(该分类下的商品的数量，wang...)
	private String popSortValue;
	//是否选中
	private boolean isSelected;
	
	public String getPopSortValue() {
		return popSortValue;
	}
	public void setPopSortValue(String popSortValue) {
		this.popSortValue = popSortValue;
	}
	public String getPopSortName() {
		return popSortName;
	}
	public void setPopSortName(String popSortName) {
		this.popSortName = popSortName;
	}
	public boolean isSelected() {
		return isSelected;
	}
	public void setSelected(boolean isSelected) {
		this.isSelected = isSelected;
	}
	
}
