<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_shoppingcart_freight_insert"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="35dip"
    android:background="@color/white"
    android:orientation="horizontal" >

    <ImageView
        android:id="@+id/iv_shoppingcart_freightchose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginBottom="15dip"
        android:src="@drawable/input_lable01_noselected" />

    <TextView
        android:id="@+id/tv_shoppingcart_freightname"
        style="@style/font_content_medium_black"
        android:layout_centerVertical="true"
        android:layout_marginLeft="10dip"
        android:layout_marginRight="10dip"
        android:layout_toRightOf="@+id/iv_shoppingcart_freightchose" />

    <TextView
        android:id="@+id/tv_shoppingcart_freight"
        style="@style/font_content_medium_red"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true" />

</RelativeLayout>