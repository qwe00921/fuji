package com.fsti.fjdicClient.bean;

/**
 * 社区商品分类信息
 * @author 王久叶 
 *
 */
public class GroupBuyingGoodsCategoryEntity {
	/**
	 *商品类别ID
	 */
	private String ID;
	public String getID() {
		return ID;
	}
	public void setID(String iD) {
		ID = iD;
	}
	/**
	 *类别等级  1:一级类别 2:二级类别 3:三级类别
	 */
	private int level;
	/**
	 * 商品类别名称
	 */
	private String name;
	/**
	 * 上级类别ID
	 */
	private String parentID;
	/**
	 * 排序位置
	 */
	private int orderPosition;
	/**
	 * 商品数量
	 */
	private int goodsCount;
	
	private String categoryImage;
	
	public String getCategoryImage() {
		return categoryImage;
	}
	public void setCategoryImage(String categoryImage) {
		this.categoryImage = categoryImage;
	}
	
	public int getGoodsCount() {
		return goodsCount;
	}
	public void setGoodsCount(int goodsCount) {
		this.goodsCount = goodsCount;
	}
	public int getLevel() {
		return level;
	}
	public void setLevel(int level) {
		this.level = level;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getParentID() {
		return parentID;
	}
	public void setParentID(String parentID) {
		this.parentID = parentID;
	}
	public int getOrderPosition() {
		return orderPosition;
	}
	public void setOrderPosition(int orderPosition) {
		this.orderPosition = orderPosition;
	}
	@Override
	public String toString() {
		return "GroupBuyingGoodsCategoryEntity [ID=" + ID + ", level=" + level
				+ ", name=" + name + ", parentID=" + parentID
				+ ", orderPosition=" + orderPosition + ", goodsCount="
				+ goodsCount + ", categoryImage=" + categoryImage + "]";
	}


	
}
