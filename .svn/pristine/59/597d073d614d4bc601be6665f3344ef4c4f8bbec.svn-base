<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <RelativeLayout style="@style/bg_top_background" >

        <Button
            android:id="@+id/btn_shoppingmall_goodslist_nobutton_back"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_alignParentLeft="true"
            android:background="@drawable/state_press_btn_top_back" />

        <TextView
            android:id="@+id/tv_shoppingmall_nobutton_title"
            style="@style/bg_top_text"
            android:text="搜索广告列表" >
        </TextView>
    </RelativeLayout>

    <TextView
        android:id="@+id/tv_goodslistnobutton_load"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:text="努力加载中，请稍候。"
        android:textColor="@color/black"
        android:textSize="20dip" />

    <ListView
        android:id="@+id/list_shoppingmall_goodslist_nobutton"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#FFFFFF"
        android:cacheColorHint="@color/white"
        android:focusable="true"
        android:focusableInTouchMode="true" >
    </ListView>

    <RelativeLayout
        android:id="@+id/rl_shoppingmall_goodslistnobutton_bottommenu"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom" >
    </RelativeLayout>
    <!--
	<LinearLayout android:id="@+id/ll_goodslist_contain"
		android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_height="fill_parent">

	</LinearLayout>    -->

</LinearLayout>